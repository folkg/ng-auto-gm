{
  "name": "auto-gm",
  "packageManager": "bun@1.2.2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "bun run vitest",
    "test:ci": "bun run vitest run --browser.headless",
    "checks": "bunx biome check",
    "fix": "bunx biome check --fix",
    "types": "tsc --noEmit",
    "all-checks": "bun run fix && bun run types && bun run test:ci",
    "deploy:dev": "firebase use dev && ng build --aot && firebase deploy --only hosting",
    "deploy:prod": "firebase use prod && ng build --prod && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.4",
    "@angular/cdk": "19.1.2",
    "@angular/common": "19.1.4",
    "@angular/compiler": "19.1.4",
    "@angular/core": "19.1.4",
    "@angular/forms": "19.1.4",
    "@angular/material": "19.1.2",
    "@angular/platform-browser": "19.1.4",
    "@angular/platform-browser-dynamic": "19.1.4",
    "@angular/router": "19.1.4",
    "arktype": "2.0.4",
    "firebase": "11.0.2",
    "rxjs": "7.8.2",
    "spacetime": "7.7.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@analogjs/platform": "1.15.1",
    "@analogjs/vite-plugin-angular": "1.15.1",
    "@analogjs/vitest-angular": "1.15.1",
    "@angular-devkit/build-angular": "19.2.9",
    "@angular/cli": "19.2.9",
    "@angular/compiler-cli": "19.1.4",
    "@biomejs/biome": "1.9.4",
    "@ngx-env/builder": "19.0.4",
    "@nx/vite": "20.8.1",
    "@testing-library/angular": "17.3.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.6.1",
    "@types/jasmine": "5.1.7",
    "@vitest/browser": "3.0.9",
    "@vitest/coverage-v8": "3.0.9",
    "jasmine-core": "5.6.0",
    "playwright": "1.52.0",
    "typescript": "5.7.3",
    "vite": "6.3.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "3.0.9"
  }
}
